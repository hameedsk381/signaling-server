[general]
configs_folder = /usr/local/etc/janus

[transports.websockets]
enabled = true
port = 8188
interface = 0.0.0.0

[transports.http]
enabled = true
port = 8088
interface = 0.0.0.0

[nat]
stun_server = stun.l.google.com
stun_port = 19302

[plugins]
disable = janus_plugin_sip janus_plugin_textroom janus_plugin_recordplay janus_plugin_streaming janus_plugin_videocall janus_plugin_nosip janus_plugin_audiobridge
